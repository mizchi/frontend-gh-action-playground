{
  "name": "action-playground",
  "version": "1.0.0",
  "main": "index.js",
  "author": "mizchi <miz404@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --mode development",
    "build": "webpack --mode production",
    "typecheck": "tsc -p . --noEmit",
    "lint": "eslint src e2e --ext='.ts,.tsx,.js'",
    "format": "prettier --write '{src,e2e}/**/*.{ts,tsx,js}'",
    "test": "run-s test:*",
    "test:unittest": "jest src --coverage --max-cpu=4",
    "test:e2e:ie": "cross-env BROWSER=ie jest e2e/runWithSelenium.test.ts",
    "test:e2e:chrome": "cross-env BROWSER=chrome jest e2e/runWithSelenium.test.ts",
    "test:e2e:firefox": "cross-env BROWSER=firefox jest e2e/runWithSelenium.test.ts",
    "test:e2e:safari": "cross-env BROWSER=safari jest e2e/runWithSelenium.test.ts",
    "test:karma:ie": "karma start --single-run --browsers ie_no_addons",
    "test:karma:chrome": "karma start --single-run --browsers Chrome",
    "test:karma:firefox": "karma start --single-run --browsers Firefox",
    "test:karma:safari": "karma start --single-run --browsers Safari"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/http-server": "^0.10.0",
    "@types/jest": "^24.0.22",
    "@types/node": "^12.12.6",
    "@types/puppeteer": "^1.20.2",
    "@types/selenium-webdriver": "^4.0.5",
    "@types/webpack": "^4.39.8",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "babel-loader": "^8.0.6",
    "chromedriver": "76",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "express": "^4.17.1",
    "geckodriver": "^1.19.1",
    "html-webpack-plugin": "^3.2.0",
    "jasmine": "^3.5.0",
    "jasmine-core": "^3.5.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.2.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-safari-launcher": "^1.0.0",
    "karma-webpack": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "puppeteer": "^2.0.0",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
